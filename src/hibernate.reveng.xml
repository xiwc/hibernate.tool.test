<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="lhjz" match-name="authorities" />
	<table-filter match-catalog="lhjz" match-name="group_authorities" />
	<table-filter match-catalog="lhjz" match-name="group_members" />
	<table-filter match-catalog="lhjz" match-name="groups" />
	<table-filter match-catalog="lhjz" match-name="users" />
	<table catalog="lhjz" name="authorities" class="Authority"></table>
	<table catalog="lhjz" name="groups" class="Group"></table>
	<table catalog="lhjz" name="users" class="User"></table>
	<table catalog="lhjz" name="group_members" class="GroupMember"></table>
	<table catalog="lhjz" name="group_authorities" class="GroupAuthority"></table>
</hibernate-reverse-engineering>